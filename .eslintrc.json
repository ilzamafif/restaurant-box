{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "no-underscore-dangle": "off",
        "operator-linebreak":"off",
        "implicit-arrow-linebreak":"off",
        "max-len": "off",
        "no-console":"off",
        "no-restricted-globals": "off"
    }
}
